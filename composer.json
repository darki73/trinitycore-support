{
    "name": "freedomcore/trinitycore-support",
    "description": "TrinityCore support Library",
    "keywords": ["freedomcore", "trinitycore", "support", "libarary"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/darki73/trinitycore-support/issues",
        "source": "https://github.com/darki73/trinitycore-support"
    },
    "authors": [
        {
            "name": "Zhivolupov Ivan",
            "email": "ivan.zhivolupov@freedomcore.ru",
            "homepage": "https://freedomcore.ru",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FreedomCore\\TrinityCore\\Support\\": "source/"
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.5.5",
        "scrutinizer/ocular": "^1.1",
        "doctrine/dbal": "^2.3",
        "phpdocumentor/phpdocumentor": "2.*",
        "squizlabs/php_codesniffer": "^3.2",
        "codedungeon/phpunit-result-printer": "^0.4.4",
        "symfony/dom-crawler": "^4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "FreedomCore\\TrinityCore\\Support\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs --standard=psr2 ./source"
    }
}
